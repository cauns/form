{
    "name": "cauns/forms",
    "description": "Effortlessly build TALL-powered forms.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "danharrin/date-format-converter": "^0.2"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Filament\\Forms\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\Forms\\FormsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
