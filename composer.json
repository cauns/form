{
    "name": "cauns/forms",
    "description": "Effortlessly build TALL-powered forms.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.11.2",
         "livewire/livewire": "^2.10",
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Filament\\Forms\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\Forms\\FormsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
